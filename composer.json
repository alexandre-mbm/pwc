{
    "name": "alexandre-mbm/pwc",
    "description": "Paperwork Consolidator",
    "type": "library",
    "keywords": ["pdf", "images", "editor"],
    "time": "2016-12-01",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Magno",
            "email": "alexandre.mbm@gmail.com"
        }
    ],
   "support": {
        "issues": "https://github.com/alexandre-mbm/pwc/issues",
        "source": "https://github.com/alexandre-mbm/pwc"
    },
    "minimum-stability": "dev",
    "require":
    {
        "php": ">=5.3.0",
        "ext-imagick": "*",
        "myokyawhtun/pdfmerger": "dev-master",
        "tecnickcom/tcpdf": "^6.2",
        "pauln/tcpdi": "master",
        "pauln/tcpdi_parser": "master",
        "fpdf/tpl": "1.2.3"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "pauln/tcpdi",
                "version": "master",
                "require":                 {
                    "tecnickcom/tcpdf": "*"
                },
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/pauln/tcpdi/archive/master.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "pauln/tcpdi_parser",
                "version": "master",
                "require": {
                    "tecnickcom/tcpdf": "*"
                },
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/pauln/tcpdi_parser/archive/master.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "fpdf/tpl",
                "version": "1.2.3",
                "require": {
                    "tecnickcom/tcpdf": "*"
                },
                "dist": {
                    "type": "zip",
                    "url": "https://www.setasign.com/downloads/63411/FPDF_TPL-1.2.3.zip"
                }
            }
        }
    ],
    "scripts": {
        "post-package-install": [
            "pwc\\Installer\\PostPackage::when_tcpdi_parser",
            "pwc\\Installer\\PostPackage::when_fpdf_tpl",
            "pwc\\Installer\\PostPackage::when_tcpdi"
        ],
        "post-package-update": "@post-package-install"
    },
    "autoload": {
        "psr-4": {
            "pwc\\Installer\\": "."
        }
    }
}
